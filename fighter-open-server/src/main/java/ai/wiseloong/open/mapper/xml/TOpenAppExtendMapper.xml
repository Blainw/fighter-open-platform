<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ai.wiseloong.open.mapper.TOpenAppExtendMapper">
    <select id="list" resultType="Map" parameterType="com.baomidou.mybatisplus.core.conditions.Wrapper">
         SELECT * FROM t_open_app_extend
         ${ew.customSqlSegment}
    </select>
    <select id="queryList" resultType="ai.wiseloong.open.pojo.vo.TOpenAppExtendVo" parameterType="com.baomidou.mybatisplus.core.conditions.Wrapper">
        SELECT * FROM t_open_app_extend
        ${ew.customSqlSegment}
    </select>
    <select id="query" resultType="ai.wiseloong.open.pojo.vo.TOpenAppExtendVo" parameterType="com.baomidou.mybatisplus.core.conditions.Wrapper">
        SELECT * FROM t_open_app_extend
        ${ew.customSqlSegment}
    </select>

    <insert id="batchInsert">
        insert into t_open_app_extend
        (id,appId,llcs,sycs,sccs,pjfs,kzdx,tenantId,createId,createTime,updateId,updateTime)values
        <foreach collection="list" separator="," item="item">
            (#{item.id},#{item.appId},#{item.llcs},#{item.sycs},#{item.sccs},#{item.pjfs},#{item.kzdx},#{item.tenantId},#{item.createId},#{item.createTime},#{item.updateId},#{item.updateTime} )
        </foreach>
    </insert>

</mapper>